                                                    <div class="TripCardFooter_Correspondance_description_wrapper">
                                                        <label class="TripCardFooter_Correspondance_description_label">@Html.DisplayFor(modelItem => item.agency.agency_name)</label>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="ant-collapse ant-collapse-icon-position-left card-collapse">
