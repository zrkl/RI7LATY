@model Tuple<R.DAL.Context.travel, IEnumerable<R.DAL.Context.travel>>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<body>

    <nav class="navbar navbar-expand-lg navbar-dark ftco_navbar bg-dark ftco-navbar-light" id="ftco-navbar">
        <div class="container">
            <a class="navbar-brand" href="index.html">RI7LATY</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#ftco-nav" aria-controls="ftco-nav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="oi oi-menu"></span> Menu
            </button>

            <div class="collapse navbar-collapse" id="ftco-nav">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item active"><a href="index.html" class="nav-link">Home</a></li>
                    <li class="nav-item"><a href="about.html" class="nav-link">About</a></li>
                    <li class="nav-item"><a href="blog.html" class="nav-link">Blog</a></li>
                    <li class="nav-item"><a href="contact.html" class="nav-link">Contact</a></li>
                    <li class="nav-item cta mr-md-2"><a href="new-post.html" class="nav-link" style="background-color: blueviolet; border-color: blueviolet;">Agency</a></li>
                    <li class="nav-item cta cta-colored"><a href="job-post.html" class="nav-link">become a collaborator</a></li>

                </ul>
            </div>
        </div>
    </nav>
    <!-- END nav -->
    <!--slider-->

    <div class="hero-wrap js-fullheight" style="background-image: url('images/6.jpg');" data-stellar-background-ratio="0.5">
        <div class="overlay"></div>
        <div class="container">
            <div class="row no-gutters slider-text js-fullheight align-items-center justify-content-start" data-scrollax-parent="true">
                <div class="col-xl-10 ftco-animate mb-5 pb-5" data-scrollax=" properties: { translateY: '70%' }">
                    <p class="mb-4 mt-5 pt-5" data-scrollax="properties: { translateY: '30%', opacity: 1.6 }"> </p>
                    <h1 class="mb-5" data-scrollax="properties: { translateY: '30%', opacity: 1.6 }">  <br><span> <br><br><br></span></h1>

                    <div class="ftco-search">
                        <div class="row">
                            <div class="col-md-12 nav-link-wrap">
                                <div class="nav nav-pills text-center" id="v-pills-tab" role="tablist" aria-orientation="vertical">
                                    <div class="col-md-12 tab-wrap">
                                        <!--here-->
                                        @using (Html.BeginForm("Index", "Home", FormMethod.Post))
                                        {
                                            @Html.AntiForgeryToken()
                                            <div class="tab-content p-4" id="v-pills-tabContent">

                                                <div class="tab-pane fade show active" id="v-pills-1" role="tabpanel" aria-labelledby="v-pills-nextgen-tab">
                                                    <form action="#" class="search-job">
                                                        <div class="row">
                                                            <div class="col-md">
                                                                <div class="form-group">
                                                                    <div class="form-field" style="text-size-adjust:auto">
                                                                        <div class="icon"></div>
                                                                        @*<input type="text" class="form-control" placeholder="Beginning">*@
                                                                        @Html.DropDownList("beginning", null, htmlAttributes: new { @class = "form-control" })
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-md">
                                                                <div class="form-group">
                                                                    <div class="form-field">
                                                                        <div class="select-wrap">
                                                                            <div class="icon"></div>
                                                                            @*<input type="text" class="form-control" placeholder="Agency">*@
                                                                            @Html.DropDownList("id_agency", ViewBag.id_agency as SelectList, htmlAttributes: new { @class = "form-control" })
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-md">
                                                                <div class="form-group">
                                                                    <div class="form-field">
                                                                        <div class="select-wrap">
                                                                            <div class="icon"></div>
                                                                            @*<input type="text" class="form-control" placeholder="Destination">*@
                                                                            @Html.DropDownList("destination", null, htmlAttributes: new { @class = "form-control" })
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-md">
                                                                <div class="form-group">
                                                                    <div class="form-field">
                                                                        <div class="icon"></div>
                                                                        <input type="date" class="form-control" placeholder="Date">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-md">
                                                                <div class="form-group">
                                                                    <div class="form-field">
                                                                        <input type="submit" value="Ceate" class="form-control btn btn-primary">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </form>
                                                </div>
                                            </div>
                                        }
                                    </div>


                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>




    <section class="ftco-section bg-light" @ViewBag.hide>
        <div class="container">
            <div class="row justify-content-center mb-5 pb-3">
                <div class="col-md-7 heading-section text-center ftco-animate">
                    <span class="subheading">Recently Added Travels</span>
                    <h2 class="mb-4"><span>Recent</span> Travels</h2>
                    <h4 class="mb-4"><span>week day</span>00/00/00</h4>
                </div>
            </div>
            <!--the div-->
            @foreach (var item in Model.Item2)
            {
                <div class="ant-card-body" style="padding: 0px;">
                    <div class="ant-row trips">
                        <div class="ant-col ant-col-24 trips-wrapper">
                            <div class="ant-card trip-wrapper ant-card-bordered">
                                <div class="ant-card-body">
                                    <div class="trip-wrapper-info">

                                    </div>
                                    <div style="height: 20px;">
                                    </div>
                                    <div class="ant-row-flex trajet-info">
                                        <div class="ant-col ant-col-6 trip-infos-wrapper">
                                            <div class="od-wrapper">
                                                <label class="label">Beginning</label>
                                                <label class="date">@Html.DisplayFor(modelItem => item.date_start)</label>
                                                <label class="station">@Html.DisplayFor(modelItem => item.ville.ville_name)</label>
                                            </div>
                                        </div>
                                        <div class="ant-col ant-col-12 trip-infos-wrapper">
                                            <div class="trip-infos">
                                                <label class="trip-duration">00:00:00</label>
                                                <ul class="progress-indicator ">
                                                    <li>
                                                        <span class="bubble"></span>
                                                    </li>
                                                    <li>
                                                        <span class="bubble"></span>
                                                    </li>
                                                </ul>
                                                <label class="trip-Correspondance">Direct</label>
                                            </div>
                                        </div>
                                        <div class="ant-col ant-col-6 trip-infos-wrapper">
                                            <div class="od-wrapper">
                                                <label class="label">Destination</label>
                                                <label class="date">@Html.DisplayFor(modelItem => item.date_arrive)</label>
                                                <label class="station">@Html.DisplayFor(modelItem => item.ville1.ville_name)</label>
                                            </div>
                                        </div>
                                        <div class="ant-divider ant-divider-horizontal" role="separator"></div>
                                    </div>
                                    <div class="ant-row">
                                        <div class="trip-footer">
                                            <div class="ant-row TripCardFooter_Wrapper">
